{
	"name": "loadcsvDataFromADLSgen2",
	"properties": {
		"linkedServiceName": {
			"referenceName": "Blogadlsgen2stor",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"Schema": {
				"type": "array"
			},
			"File": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().File",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat(dataset().File, '/', utcnow('yyyy'), '/', utcnow('MM'), '/', utcnow('dd')) ",
					"type": "Expression"
				},
				"fileSystem": "sqldbdata"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"quoteChar": "\""
		},
		"schema": [],
		"structure": {
			"value": "@dataset().Schema",
			"type": "Expression"
		}
	}
}